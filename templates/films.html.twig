{% extends "base.html.twig" %}

{% block title %}{{titleFromController}}{% endblock %}

{% block body %}

<table>
<thead>
    <th>Id</th>
    <th>Titre</th>
    <th>Duree</th>
</thead>
<tbody>
    {% for film in films %}
        <tr>
            <td>{{film.id}}</td>
            <td>{{film.titre}}</td>
            <td>{{film.duree}}</td>
        </tr>  
    {% endfor %}
</tbody>
</table>

{% endblock %}